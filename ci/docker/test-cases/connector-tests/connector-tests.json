	"item": [
		{
			"name": "well known",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test(\"Protocol versions structure is valid\", function () {\r",
							"    pm.expect(response.protocolVersions).to.be.an('array').that.is.not.empty;\r",
							"\r",
							"    response.protocolVersions.forEach(protocol => {\r",
							"        pm.expect(protocol).to.have.property('version').that.equals('2025-1');\r",
							"        pm.expect(protocol).to.have.property('binding').that.equals('HTTPS');\r",
							"    });\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
