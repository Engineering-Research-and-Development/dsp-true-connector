					"name": "Login to get JWT token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response contains access token\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.data.accessToken).to.exist;\r",
									"    pm.collectionVariables.set(\"accessToken\", jsonData.data.accessToken);\r",
									"});\r",
									"\r",
									"pm.test(\"Response contains refresh token\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.data.refreshToken).to.exist;\r",
									"    pm.collectionVariables.set(\"refreshToken\", jsonData.data.refreshToken);\r",
									"});\r",
									"\r",
									"pm.test(\"Response contains user info\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.data.email).to.eql(\"admin@mail.com\");\r",
									"    pm.expect(jsonData.data.role).to.exist;\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
